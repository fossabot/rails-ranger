{
  "plugins": [
    "plugins/markdown",
    "plugins/underscore"
  ],

  "opts": {
    "template": "node_modules/minami",
    "destination": "./docs/",
    "encoding": "utf8"
  },

  "source": {
    "include": ["src", "README.md"],
    "includePattern": ".js$"
  },

  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true,
    "useLongnameInNav": false,
    "showInheritedInNav": true
  }
}
